<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <title>Dashboard Users</title>
</head>

<body>
    <div class="container-fluid col-6  my-5 p-3 bg-light text-dark shadow-lg mb-5 bg-body rounded">
        <h1>Dashboard Users</h1>
        <div class="d-inline-flex p-2 bd-highlight">
            <a href="/"><button type="button" class="btn btn-outline-primary">Landing Page</button></a>
        </div>
        <div class="d-inline-flex p-2 bd-highlight">
            <a href="/signup"><button type="button" class="btn btn-outline-primary">Create New User</button></a>
        </div>
        <div class="my-3">
        <ol>
        <% users.forEach((user)=> { %>
           
           
                    <div>
                        <li>
                            <p>Username: <%= user.username %></p>
                            <p>Name: <%= user.user_biodata.name %></p>
                            <p>Result: <%= user.user_history.result %></p>
                        </li>
                        <a href="/users/<%= user.id %>"><button type="button" class="btn btn-primary">Profile</button></a>
                        <a href="/users/update/<%= user.id %>"><button type="button" class="btn btn-success">Update</button></a>
                        <a href="/users/delete/<%= user.id %>"><button type="button" class="btn btn-warning">Remove</button></a>
                        <hr>
                    </div>
                <% }) %>
         
        </ol>
        </div>
    </div>
</body>

</html>